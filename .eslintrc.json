{
  "parser": "babel-eslint",
  "extends": [
    "react-app",
    "prettier",
    "prettier/react",
    "plugin:react-native/all"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": ["react", "prettier", "react-native"],
  "rules": {
    "prettier/prettier": "error",
    "react-native/no-inline-styles": 1,
    "react-native/no-color-literals": 0
  }
}
